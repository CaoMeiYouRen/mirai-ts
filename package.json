{
  "name": "mirai-ts",
  "version": "2.3.5",
  "description": "Mirai TypeScript SDK",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "demo": "node demo/index.js",
    "demo:ts": "ts-node-dev demo/index.ts",
    "prepublishOnly": "npm run build",
    "build": "tsup src/index.ts",
    "docs:build": "typedoc",
    "docs:dev": "cd docs && live-server",
    "lint": "eslint --fix . --ext .ts",
    "mcl": "cd mcl && ./mcl",
    "watch": "tsup src/index.ts --watch",
    "test": "ts-node-dev test/index.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YunYouJun/mirai-ts"
  },
  "homepage": "https://www.yunyoujun.cn/mirai-ts/",
  "keywords": [
    "mirai",
    "http",
    "api",
    "sdk",
    "typescript",
    "node",
    "js"
  ],
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "AGPL-3.0",
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.18.9",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.23",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "husky": "7.0.4",
    "js-yaml": "^4.1.0",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "ts-node-dev": "^1.1.8",
    "tsup": "^5.12.1",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@yunyoujun/logger": "^0.2.1",
    "axios": "^0.26.1",
    "chalk": "4",
    "form-data": "^4.0.0",
    "ws": "^8.5.0"
  }
}
