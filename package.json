{
  "name": "mirai-ts",
  "version": "2.4.4",
  "packageManager": "pnpm@7.12.2",
  "description": "Mirai TypeScript SDK",
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "AGPL-3.0",
  "homepage": "https://www.yunyoujun.cn/mirai-ts/",
  "repository": {
    "type": "git",
    "url": "https://github.com/YunYouJun/mirai-ts"
  },
  "keywords": [
    "mirai",
    "http",
    "api",
    "sdk",
    "typescript",
    "node",
    "js"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts",
    "demo": "node demo/index.js",
    "demo:ts": "tsx watch demo/index.ts",
    "dev": "npm run watch",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "docs:build": "typedoc",
    "docs:dev": "cd docs && live-server",
    "lint": "eslint .",
    "mcl": "cd mcl && ./mcl",
    "watch": "tsup src/index.ts --watch",
    "release": "bumpp --commit --push --tag",
    "test": "tsx watch test/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@yunyoujun/logger": "^0.2.3",
    "axios": "^1.3.4",
    "chalk": "4",
    "form-data": "^4.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.38.2",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.15.11",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "bumpp": "^9.1.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "husky": "8.0.3",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "tsup": "6.7.0",
    "tsx": "^3.12.6",
    "typedoc": "^0.23.28",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  }
}
