{
  "name": "mirai-ts",
  "version": "2.3.1",
  "description": "Mirai TypeScript SDK",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "demo": "node demo/index.js",
    "demo:ts": "ts-node-dev demo/index.ts",
    "prepublishOnly": "npm run build",
    "build": "tsup src/index.ts",
    "docs:build": "typedoc",
    "docs:dev": "cd docs && live-server",
    "lint": "eslint --fix . --ext .ts && prettier --write .",
    "mcl": "cd mcl && ./mcl",
    "watch": "rm -rf dist && tsc -w",
    "test": "ts-node-dev test/index.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YunYouJun/mirai-ts"
  },
  "homepage": "https://www.yunyoujun.cn/mirai-ts/",
  "keywords": [
    "mirai",
    "http",
    "api",
    "sdk",
    "typescript",
    "node",
    "js"
  ],
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "AGPL-3.0",
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.12",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "dotenv": "^14.3.2",
    "eslint": "^8.7.0",
    "husky": "7.0.4",
    "js-yaml": "^4.1.0",
    "lint-staged": "^12.3.2",
    "prettier": "^2.5.1",
    "ts-node-dev": "^1.1.8",
    "tsup": "^5.11.11",
    "typedoc": "^0.22.11",
    "typescript": "~4.5.5"
  },
  "dependencies": {
    "@yunyoujun/logger": "^0.2.0",
    "axios": "^0.25.0",
    "chalk": "^5.0.0",
    "form-data": "^4.0.0",
    "ws": "^8.4.2"
  }
}
