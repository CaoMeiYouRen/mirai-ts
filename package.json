{
  "name": "mirai-ts",
  "version": "2.0.0-beta.1",
  "description": "Mirai TypeScript SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "demo": "node demo/index.js",
    "demo:ts": "ts-node-dev demo/index.ts",
    "prepublishOnly": "npm run build",
    "build": "rm -rf dist && tsc",
    "docs:build": "typedoc",
    "docs:dev": "cd docs && live-server",
    "lint": "eslint --fix . --ext .ts && prettier --write .",
    "mcl": "cd mcl && ./mcl",
    "watch": "rm -rf dist && tsc -w",
    "test": "ts-node-dev test/index.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YunYouJun/mirai-ts"
  },
  "homepage": "https://www.yunyoujun.cn/mirai-ts/",
  "keywords": [
    "mirai",
    "http",
    "api",
    "sdk",
    "typescript",
    "node",
    "js"
  ],
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "AGPL-3.0",
  "devDependencies": {
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^16.3.1",
    "@types/ws": "^7.4.6",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "husky": "7.0.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "ts-node-dev": "^1.1.8",
    "typedoc": "^0.21.3",
    "typescript": "~4.3.5"
  },
  "dependencies": {
    "@yunyoujun/logger": "^0.1.4",
    "axios": "^0.21.1",
    "chalk": "^4.1.2",
    "form-data": "^4.0.0",
    "ws": "^7.5.3"
  }
}
