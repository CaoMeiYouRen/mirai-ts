{
  "name": "mirai-ts",
  "version": "1.1.1",
  "description": "Mirai TypeScript SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "demo": "node demo/index.js",
    "demo:ts": "ts-node-dev demo/index.ts",
    "prepublishOnly": "npm run build",
    "build": "rm -rf dist && tsc",
    "docs:build": "typedoc",
    "docs:dev": "cd docs && live-server",
    "lint": "eslint --fix . --ext .ts && prettier --write .",
    "watch": "rm -rf dist && tsc -w",
    "test": "ts-node-dev test/index.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YunYouJun/mirai-ts"
  },
  "homepage": "https://www.yunyoujun.cn/mirai-ts/",
  "keywords": [
    "mirai",
    "http",
    "api",
    "sdk",
    "typescript",
    "node",
    "js"
  ],
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "AGPL-3.0",
  "devDependencies": {
    "@types/node": "^15.12.2",
    "@types/ws": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "husky": "6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "ts-node-dev": "^1.1.6",
    "typedoc": "^0.20.36",
    "typescript": "4.2"
  },
  "dependencies": {
    "@yunyoujun/logger": "^0.1.2",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "form-data": "^4.0.0",
    "ws": "^7.4.6"
  }
}
